	    -> (FirstName, LastName, Address, City, State, Zip, PhoneNum, EmailId)
	    -> VALUES
	    -> ('Dipali', 'Magare', 'Amalner', 'Amalner', 'Maharashtra', 425401, 7788991122,
	 ‘dipali.magare@gmail.com');
